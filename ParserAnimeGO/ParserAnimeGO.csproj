<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
	<Version>1.1.0-beta.1</Version>
	<Nullable>enable</Nullable>
    <Title>ParserAnimeGO</Title>
    <Authors>kamaksimka</Authors>
    <Description>Parser for the site animego.org.</Description>
    <PackageTags>Parser, Anime, AnimeGO</PackageTags>
    <RepositoryUrl>https://github.com/KaMaksimKa/ParserAnimeGO</RepositoryUrl>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Copyright>MIT License</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AngleSharp" Version="1.0.4" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
  </ItemGroup>

</Project>
